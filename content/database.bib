@Article{HollidayLitak2019CAaMI,
  author    = {Holliday, Wesley H. and Litak, Tadeusz},
  journal   = {The Review of Symbolic Logic},
  title     = {{Complete Additivity and Modal Incompleteness}},
  year      = {2019},
  issn      = {1755-0203, 1755-0211},
  month     = sep,
  number    = {3},
  pages     = {487--535},
  volume    = {12},
  abstract  = {In this article, we tell a story about incompleteness in modal logic. The story weaves together an article of van Benthem (1979), “Syntactic aspects of modal incompleteness theorems,” and a longstanding open question: whether every normal modal logic can be characterized by a class of completely additive modal algebras, or as we call them,                                                 VV\{{\textbackslash}cal V\}                           -baos. Using a first-order reformulation of the property of complete additivity, we prove that the modal logic that starred in van Benthem’s article resolves the open question in the negative. In addition, for the case of bimodal logic, we show that there is a naturally occurring logic that is incomplete with respect to                                                 VV\{{\textbackslash}cal V\}                           -baos, namely the provability logic                                                 GLBGLBGLB                            (Japaridze, 1988; Boolos, 1993). We also show that even logics that are unsound with respect to such algebras do not have to be more complex than the classical propositional calculus. On the other hand, we observe that it is undecidable whether a syntactically defined logic is                                                 VV\{{\textbackslash}cal V\}                           -complete. After these results, we generalize the Blok Dichotomy (Blok, 1978) to degrees of                                                 VV\{{\textbackslash}cal V\}                           -incompleteness. In the end, we return to van Benthem’s theme of syntactic aspects of modal incompleteness.},
  doi       = {10.1017/S1755020317000259},
  file      = {:HollidayLitak2019CAaMI - Complete Additivity and Modal Incompleteness.pdf:PDF},
  language  = {en},
  publisher = {Cambridge University Press},
  urldate   = {2021-03-03},
}

