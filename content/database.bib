@InCollection{Bacon2021OaP,
  author    = {Bacon, Andrew},
  publisher = {Routledge},
  title     = {Opacity and {Paradox}},
  year      = {2021},
  isbn      = {9780429030208},
  abstract  = {In 1963 Prior proved a theorem that places surprising constraints on the logic of intentional attitudes, like ‘thinks that’, ‘hopes that’, ‘says that’ and ‘fears that’. Paraphrasing it in English, and applying it to ‘thinks’, it states: If, at t, I thought that I didn’t think a truth at t, then there is both a truth and a falsehood I thought at t.                This chapter explores a response to this paradox that exploits the opacity of attitude verbs, exemplified in this case by the operator ‘I thought at t that’, to block Prior’s derivation. According to this picture, both Leibniz’s law and existential generalization fail in opaque contexts. In particular, one cannot infer from the fact that I’m thinking at t that I’m not thinking a truth at t, that there is a particular proposition such that I am thinking it at t. Moreover, unlike some approaches to this paradox (see Bacon et al., 2016) the failure of existential generalization is not motivated by the idea that certain paradoxical propositions do not exist, for this view maintains that there is a proposition that I’m not thinking a truth at t. Several advantages of this approach over the non-existence approach are discussed, and models demonstrating the consistency of this theory are provided. Finally, the resulting considerations are applied to the liar paradox, and are used to provide a non-standard justification of a classical gap theory of truth. One of the main challenges for this sort of theory—to explain the point of assertion, if not to assert truths—can be met within this framework.},
  file      = {Full Text PDF:https\://s3-euw1-ap-pe-df-pch-content-store-p.s3.eu-west-1.amazonaws.com/9780429030208/b80c22c2-a563-4702-b2b4-666c2451fe14/chapters/chapter10.pdf?AWSAccessKeyId=ASIAQFVOSJ57ZJFD5Q3M&Expires=1646249350&Signature=1cdvFaXHuOy1SI51%2BB3g2H9SmtY%3D&response-content-disposition=attachment%3B%20filename%3D%229780429030208_chapterpdf.pdf%22&x-amz-security-token=IQoJb3JpZ2luX2VjEMX%2F%2F%2F%2F%2F%2F%2F%2F%2F%2FwEaCWV1LXdlc3QtMSJHMEUCIQD30czCdp0ziELWfIRD4FJbK%2BdGFoVsibYdbRJ8hKY4uAIgBHpv15cWpUB49xMsYZoT%2FNJvWoi6e4O5W7qKbx6wlHAq%2BgMILhADGgwwMTIxNzcyNjQ1MTEiDO1JAg4eJiESm8%2BPzirXAxPo1RDuDVUAK9OJ7UEffueacMHZn%2F0PMH2Th9obud7%2B0qglvPz%2BrrFTt32xxTAW9Z96q6pjUsBCVEgz0ukhHmi37vFzETNFxPEBOikZxqPIL6X1YRk7iua9xFUlhKY6qcDCNbq5o8oFRGCJlecygI1yLjuUC69xfwCdSSb7ph%2FiYR%2FLODUiYbi4eEx%2BRqpVG1tI7bb1Z70ZYeK0sqFcnA%2FzBNNqqn%2BYL7rgyrB8g3iRMNH%2BZiofEucZZmqF5W9EioYW8STfZouq2ikmMAsqvFH6oX4khXrYg7kfrvoKQLSpUKS6%2B1Qiv77q2ATShFLQTIiiDozrVvdYT44kgjiZdotN%2BHkyA%2FiiCBKhNBmniqXqA9J3g344sVQf99v4Mdgzujbko7TAkbLOebvZSE0VfnV9oqfV4QNTwEcl0DhLGp7AaWsiU1Hl6bwXIfzeSAnuMPAQH7Gl1a9F%2FOKZFMeDu75nQzLXgtY%2BOU58jc87VIQx7T7it1QPE%2BoXlOQJXDCkijEOiQeM9bDlAJ8mEZhr8Shg7FX%2FSnYxVyKDMy9W0QTHFxErLk%2FEvU2YIfe4JfiTRFd1N0E85U9C%2BKHXJ4ClPrgJA40K85%2FCa2feIY8WUkbFjJDaCZhEwzDy2f2QBjqlAehqp80iuFlfnZUXSuwIzllR9FbtZg6GPe7Vd2AYARPJ8OK5VUEQTKv5ogIw0%2BW3NzbnX5%2BQgmSZ7WfD6znkO7WXff3AxdG%2Fb%2BGXWs7o2iasXGpgcDcSYMvMTM%2FDn8WHXCv5%2Fsa16iLuGZu3MzRgGeJLz5%2BTw%2BH6qdLE0XwASaVB5iMm%2BfLnGSU9BQWrvbjDs5chqF0ejp4CNXc0p42KSoVwBiQQYQ%3D%3D:application/pdf;:Bacon2021OaP - Opacity and Paradox.pdf:PDF},
