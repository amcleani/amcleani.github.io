@Article{DiversParry2018AMDDaDR,
  author   = {Divers, John and Parry, John J},
  journal  = {Analysis},
  title    = {Advanced modalizing de dicto and de re},
  year     = {2018},
  issn     = {0003-2638},
  month    = jul,
  number   = {3},
  pages    = {415--425},
  volume   = {78},
  abstract = {Lewis’ (1968, 1986) analysis of modality faces a problem in that it appears to confer unintended truth values to certain modal claims about the pluriverse: e.g. ‘It is possible that there are many worlds’ is false when we expect truth. This is the problem of advanced modalizing. Divers (1999, 2002) presents a principled solution to this problem by treating modal modifiers as semantically redundant in some such cases. However, this semantic move does not deal adequately with advanced de re modal claims. Here, we motivate and detail a comprehensive semantics (a la Lewis 1968) for advanced modalizing de dicto and de re. The generalized semantic feature of the initial solution is not redundancy but absence from counterpart-theoretic translations of world-constrictions.},
  doi      = {10.1093/analys/anx129},
  file     = {:DiversParry2018AMDDaDR.pdf:PDF},
  groups   = {Advanced modalizing},
  url      = {https://doi.org/10.1093/analys/anx129},
