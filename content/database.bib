@Article{Stronkowski2018OtBETfUC,
  author   = {Stronkowski, Michal M.},
  journal  = {arXiv:1810.09286 [math]},
  title    = {{On the {Blok}-{Esakia} Theorem for Universal Classes}},
  year     = {2018},
  month    = oct,
  note     = {arXiv: 1810.09286},
  abstract = {The Blok-Esakia theorem states that there is an isomorphism from the lattice of intermediate logics onto the lattice of normal extensions of Grzegorczyk modal logic. The extension for multi-conclusion consequence relations was obtained by Emil Je{\textbackslash}v\{r\}{\textbackslash}'abek as an application of canonical rules. We show that Je{\textbackslash}v\{r\}{\textbackslash}'abek's result follows already from Blok's algebraic proof. We also prove that the properties of strong structural completeness and strong universal completeness are preserved and reflects by the aforementioned isomorphism. These properties coincide with structural completeness and universal completeness respectively for single-conclusion consequence relations and, in particular, for logics.},
  file     = {:Stronkowski2018OtBETfUC - On the Blok Esakia Theorem for Universal Classes.pdf:PDF},
