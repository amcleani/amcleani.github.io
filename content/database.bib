@Article{Ding2021OtLoBaPQ,
  author   = {Ding, Yifeng},
  journal  = {Journal of Philosophical Logic},
  title    = {On the {Logic} of {Belief} and {Propositional} {Quantification}},
  year     = {2021},
  issn     = {1573-0433},
  month    = oct,
  number   = {5},
  pages    = {1143--1198},
  volume   = {50},
  abstract = {We consider extending the modal logic KD45, commonly taken as the baseline system for belief, with propositional quantifiers that can be used to formalize natural language sentences such as “everything I believe is true” or “there is something that I neither believe nor disbelieve.” Our main results are axiomatizations of the logics with propositional quantifiers of natural classes of complete Boolean algebras with an operator (BAOs) validating KD45. Among them is the class of complete, atomic, and completely multiplicative BAOs validating KD45. Hence, by duality, we also cover the usual method of adding propositional quantifiers to normal modal logics by considering their classes of Kripke frames. In addition, we obtain decidability for all the concrete logics we discuss.},
  doi      = {10.1007/s10992-021-09595-8},
  file     = {Full Text PDF:https\://link.springer.com/content/pdf/10.1007%2Fs10992-021-09595-8.pdf:application/pdf;:Ding2021OtLoBaPQ.pdf:PDF},
